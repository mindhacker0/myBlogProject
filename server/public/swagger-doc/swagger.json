    "host": "192.168.1.147:8099",
    "basePath": "/",
    "tags": [
        {
            "name": "home",
            "description": "首页展示的接口",
            "externalDocs": {
                "description": "首页的推荐内容接口"
            }
        },
        {
            "name": "Public",
            "description": "页面的公共接口",
            "externalDocs": {
